            <OutputItemType>Contract</OutputItemType>
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </PackageReference>
    </ItemGroup>
    
    <ItemGroup>
        <Protobuf Include="Protobuf/stub/*.proto">
            <GrpcOutputOptions>stub</GrpcOutputOptions>
            <Access>Internal</Access>
        </Protobuf>
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Include="assets\contract\*.*" />
    </ItemGroup>

</Project>
